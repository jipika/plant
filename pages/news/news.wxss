/* pages/news/news.wxss */
.van-nav-bar {
  background-color: #edf1f2 !important;
  z-index: 999 !important;
}
.van-search {
  background-color: #edf1f2 !important;
}
.searchHigh {
  color: #409eff;
}
.img:first-child {
  margin-top: 10px;
}
.img {
  width: 3rem;
  height: 3rem;
  border-radius: 50%;
  margin-bottom: 10px;
  margin-left: 20px;
  margin-top: 20px;
}
.tup {
  display: block;
  border-top: 0.5px solid #cccccc;
}
.tup:last-child {
  border-bottom: 0.5px solid #cccccc;
}
.box {
  position: relative;
  border-top: 0.5px solid #cccccc;
}
.box:first-child {
  border: 0px solid #000;
}
.box:last-child {
  border-bottom: 0.5px solid #cccccc;
}
.pantTop text {
  position: absolute;
  top: 3.5vh;
  left: 15vh;
}
.tab {
  width: 100%;
  height: 12vh;
}
.van-divider {
  margin: 0 20px !important;
}
.van-nav-bar__title {
  color: #a0a1a2 !important;
}
.tup1 {
  transition: all 0.3s linear;
  background-color: #dedede;
  -webkit-transition: all 0.3s linear;
  -moz-transition: all 0.3s linear;
  -ms-transition: all 0.3s linear;
  -o-transition: all 0.3s linear;
}
.box1 {
  transition: all 0.3s linear;
  background-color: #dedede;
  -webkit-transition: all 0.3s linear;
  -moz-transition: all 0.3s linear;
  -ms-transition: all 0.3s linear;
  -o-transition: all 0.3s linear;
}
.bg {
  margin-top: 40px;
  width: 100%;
}
